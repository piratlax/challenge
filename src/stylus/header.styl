@import './menu.styl'

.header
    border: 1px solid blue
    background white
    position fixed
    left 0
    right 0
    padding 2em 0
    &-logo
        display inline-flex
        width 150px
        height 40px
        border: 1px solid 
        background url('../images/logo-bookmark.svg') no-repeat center 
    &-content
        display flex
        justify-content: space-between
        align-items: center